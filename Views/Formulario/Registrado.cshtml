<h3>Datos registrados de usuario</h3>
<table class="table">
    <tr>
        <td>ID:</td>
        <td>@ViewBag.cuenta.Id</td>
    </tr>
    <tr>
        <td>Usuario:</td>
        <td>@ViewBag.cuenta.Nombre</td>
    </tr>
    <tr>
        <td>Password:</td>
        <td>@ViewBag.cuenta.Password</td>
    </tr>
    <tr>
        <td>Descripcion:</td>
        <td>@ViewBag.cuenta.Descripcion</td>
    </tr>
    <tr>
        <td>Disponible</td>
        <td>@ViewBag.cuenta.Disponible</td>
    </tr>
    <tr>
        <td>Genero:</td>
        <td>@ViewBag.cuenta.genero</td>
    </tr>
    <tr>
        <td>Lenguajes</td>
        <td>
            @foreach (var len in ViewBag.cuenta.Lenguajes)
            {
                @len <br>
            }
        </td>
    </tr>
    <tr>
        <td>Cargo</td>
        <td>@ViewBag.cuenta.cargo</td>
    </tr>
    </tableclass>
